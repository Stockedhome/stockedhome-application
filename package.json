{
  "name": "stockedhome-application",
  "private": true,
  "devDependencies": {
    "eslint": "^9.4.0",
    "turbo": "^1.13.3",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "schema": "src/db/schema.prisma"
  },
  "scripts": {
    "dev-expo:android": "cd src/platforms/expo && pnpm run android",
    "dev-expo:ios": "cd src/platforms/expo && pnpm run ios",
    "dev-expo:dev": "cd src/platforms/expo && pnpm run dev",
    "predev-next": "supabase start --ignore-health-check",
    "dev-next": "next dev src/platforms/next",
    "codegen": "pwsh -File ./codegen/database-stuff.ps1 && tsx codegen/index.mjs",
